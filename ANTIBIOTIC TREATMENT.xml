											codeSystem="%%HL7_CODE_FOR_THE_VOCABULARY%%" 
											codeSystemName="%%NAME_OF_THE_VOCABULARY_USED%%" 
											displayName="%%LITERAL_OF_THE_USED_TERM%%"/>
											<!-- Name of the generic product -->
											<translation code="%%GENERIC_COREDATASET_CONCEPT_CODE%%" 
													codeSystem="%%HL7_CODE_FOR_THE_VOCABULARY%%" 
													codeSystemName="%%NAME_OF_THE_VOCABULARY_USED%%" 
													displayName="%%LITERAL_OF_THE_USED_TERM%%"/>
